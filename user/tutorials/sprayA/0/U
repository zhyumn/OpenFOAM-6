FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    /*
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    */
    inlet
    {
        type uniformFixedValue;
        uniformValue table
        (
            (0 (0 0 0))
            (2e-7 (9.010213594  0 0))
            (4e-7 (18.01990852  0 0))
            (6e-7 (27.02908867  0 0))
            (8e-7 (36.03775792  0 0))
            (1e-6 (45.04592015  0 0))
            (10e-6 (45.04592015  0 0))
        );
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            noSlip;
    }
}


