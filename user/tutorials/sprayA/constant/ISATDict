FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      ISATDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



THvfctree_XrhoP
{
    checkInterval  20;
    maxDepthFactor   2;

    maxNLeafs 1000; //tuning using activated leafs
    NtimeTag 10;
    maxLeafsize (0.3 5 2e6);
    toleranceOut (1e-2 10 1e-2 1);//(1e-0 1e-1 1e-3 1e-1);//BUG input!!(1e-10 1e-10 1e-10);//(1e-0 1e-0 1e-3);//(1e-1 1e-8 1e-3);
    initToleranceIn (1e-4 1e-2 10);
    scaleIn (1  10 1e7);//(1 1 1 1);//(1 1 2.3e+07 500);
}
Erhovfctree_XTP
{

    checkInterval  20;
    maxDepthFactor   2;

    maxNLeafs 1000; //tuning using activated leafs
    NtimeTag 10;
    maxLeafsize (0.3 1e6 50);
    toleranceOut (1e-0 1e-5 1e-2 5);//BUG input!!(1e-10 1e-10 1e-10);//(1e-0 1e-0 1e-3);//(1e-1 1e-8 1e-3);
    initToleranceIn (1e-4 1e-1 1);
    scaleIn (1 100 100000);//(1 1 1 1);//(1 1 2.3e+07 500);
}

