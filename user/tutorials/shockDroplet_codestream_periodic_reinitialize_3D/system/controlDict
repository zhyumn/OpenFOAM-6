FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            ("libISATVLERhoThermo.so");

application     rhoCentralRealgasFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.0003;

deltaT          1e-6;

writeControl    adjustableRunTime;

writeInterval   1e-5;

purgeWrite      0;

writeFormat     ascii;//binary;

writePrecision  12;//6;

writeCompression off;

timeFormat      general;

timePrecision   6;

adjustTimeStep  yes;

maxCo           0.1;

runTimeModifiable yes;



